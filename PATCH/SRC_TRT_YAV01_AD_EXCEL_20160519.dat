Funprog TRANSFORM_HTML(VALEUR)
Value Char VALEUR()

Local Char RET(200)
Local Integer I
Local Char SRC(10), DES(10)

RET = VALEUR

For SRC="&amp;"
 Gosub TRANSFORME
 I = instr(1,RET,SRC)
 While I>0
  RET= seg$(RET,1,I-1) + DES + seg$(RET,I+len(SRC),len(RET))
  I = instr(1,RET,SRC)
 Wend
Next
#Call MESSAGE("ol"-RET) From GESECRAN
End RET

$TRANSFORME
Case SRC
 When "&amp;" : DES = "&"
Endcase

Return

