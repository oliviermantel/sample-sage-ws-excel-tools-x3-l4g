# Pour le budget on commence toujours à la période 1 : pas d'actuals
[L]TABVAL(ZI,246) = "=SUM(RC213:OFFSET(RC213,0,MIN(RC227-1,11)))"
[L]TABVAL(ZI,247) = "=IF(AND(RC227>0,RC227<12),SUM(OFFSET(RC213,0,RC227):RC224),0)"

